    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [],
   "source": [
    "N=1000\n",
    "L=1\n",
    "x=np.linspace(0,L,N)\n",
    "def wf(x,n,L=1):\n",
    "    return (np.sqrt(2/L)*np.sin(np.pi*x*n/L))\n",
    "def dot(wfn1,wfn2):\n",
    "    return np.sum(wfn1*wfn2)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [
    {
     "data": {
